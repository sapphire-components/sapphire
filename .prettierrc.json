{
	"$schema": "https://json.schemastore.org/prettierrc",
	"printWidth": 200,
	"semi": true,
	"singleQuote": true,
	"tabWidth": 2,
	"useTabs": true,
	"trailingComma": "all",
	"bracketSpacing": true,
	"endOfLine": "lf",
	"proseWrap": "preserve",
	"css": {
		"format": {
			"remove_blank_lines": true
		}
	},
	"scss": {
		"format": {
			"remove_blank_lines": true
		}
	}
}
